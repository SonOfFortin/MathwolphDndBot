        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MathwolphDndBot"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btn_connect" Content="Button" HorizontalAlignment="Left" Margin="80,49,0,0" VerticalAlignment="Top" Click="btn_connect_Click"/>
        <Label x:Name="lbl_chanel" Content="Nom de la chaîne:" HorizontalAlignment="Left" Margin="139,46,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txt_chanel" HorizontalAlignment="Left" Margin="260,54,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lbl_token" Content="Jeton D'accès:" HorizontalAlignment="Left" Margin="400,46,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txt_token" HorizontalAlignment="Left" Margin="500,54,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
